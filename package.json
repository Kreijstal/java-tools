{
  "name": "jvm_tools",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.test.js",
    "build:java": "javac sources/*.java",
    "generate": "npm run build:java && node scripts/generateData.js",
    "build": "npm run generate && node scripts/buildSite.js",
    "clean": "find sources -name '*.class' -delete && rm -rf dist",
    "pretest": "npm run build:java",
    "ci": "npm run build && npm test",
    "health-check": "node scripts/health-check.js"
  },
  "keywords": [],
  "author": "kreijstal",
  "license": "AGPLv3",
  "description": "Java bytecode analysis and JVM implementation tools",
  "dependencies": {
    "jvm_parser": "^0.0.3",
    "tape": "^5.9.0"
  }
}
